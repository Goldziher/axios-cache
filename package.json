{
    "name": "axios-cache",
    "version": "1.0.0",
    "private": true,
    "description": "Configurable cache adapter for axios, works in the browser and node",
    "keywords": [
        "axios",
        "cache",
        "node",
        "browser",
        "cache-control"
    ],
    "author": "Na'aman Hirschfeld",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Goldziher/axios-cache.git"
    },
    "bugs": {
        "url": "https://github.com/Goldziher/axios-cache/issues"
    },
    "homepage": "https://github.com/Goldziher/axios-cache#readme",
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "rimraf dist && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
        "format": "prettier --write '**/*.{js,md,json,yaml,yml}'",
        "lint": "eslint --fix '**/*.ts'",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "prepare": "husky install"
    },
    "peerDependencies": {
        "axios": "*"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^8.3.0",
        "@sprylab/eslint-config": "^1.6.2",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.6",
        "@types/node-localstorage": "^1.3.0",
        "axios": "^0.24.0",
        "eslint": "^8.1.0",
        "husky": ">=7",
        "jest": "^27.3.1",
        "lint-staged": ">=11",
        "prettier": "^2.4.1",
        "rimraf": "^3.0.2",
        "rollup": "2.59.0",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "@tool-belt/type-predicates": "^1.2.0",
        "cache-control-parser": "^2.0.2",
        "node-localstorage": "^2.2.1"
    },
    "lint-staged": {
        "*.ts": "eslint --fix",
        "*.{js,md,json,yaml,yml}": "prettier --write"
    }
}
